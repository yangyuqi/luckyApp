package com.yunqilai.consumer.luckyapp.HomePage.Model;

import java.util.List;

/**
 * Created by Administrator on 2017/6/29.
 */

public class ThreeChannelInfoBean {
    private String title ;
    private String content ;
    private String id ;
    private int deleteStatus ;
    private List<ThreeChannelInfoGoodsBean> goodsList ;

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public int getDeleteStatus() {
        return deleteStatus;
    }

    public void setDeleteStatus(int deleteStatus) {
        this.deleteStatus = deleteStatus;
    }

    public List<ThreeChannelInfoGoodsBean> getGoodsList() {
        return goodsList;
    }

    public void setGoodsList(List<ThreeChannelInfoGoodsBean> goodsList) {
        this.goodsList = goodsList;
    }

    public ThreeChannelInfoBean(String title, String content, String id, int deleteStatus, List<ThreeChannelInfoGoodsBean> goodsList) {

        this.title = title;
        this.content = content;
        this.id = id;
        this.deleteStatus = deleteStatus;
        this.goodsList = goodsList;
    }
}
