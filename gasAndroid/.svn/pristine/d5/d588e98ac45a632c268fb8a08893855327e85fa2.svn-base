package com.yunqilai.consumer.luckyapp.UserCenter.View;

import android.support.v7.widget.RecyclerView;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;

import com.yunqilai.consumer.R;
import com.yunqilai.consumer.luckyapp.Common.View.Vu;

/**
 * Created by Administrator on 2017/6/7.
 */

public class EveryOrderView implements Vu {

    private View view ;
    private RecyclerView rv ;

    public RecyclerView getRv() {
        return rv;
    }

    @Override
    public void init(LayoutInflater inflater, ViewGroup container) {
        view = inflater.inflate(R.layout.every_order_layout,null);
        rv = (RecyclerView) view.findViewById(R.id.every_order_rv);
    }

    @Override
    public View getView() {
        return view;
    }
}
